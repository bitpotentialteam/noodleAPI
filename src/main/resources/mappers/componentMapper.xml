<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.noodle.mapper.ComponentMapper">
	<insert id="create">
		insert into tbl_component(name, category, component, time)
		values(#{name}, #{category}, #{component}, #{time})
	</insert>

	<delete id="delete" parameterType="java.lang.String">
		delete from tbl_component where #{name}
	</delete>
</mapper>